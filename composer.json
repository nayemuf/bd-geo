{
  "name": "nayemuf/bd-geo",
  "description": "Bangladesh Administrative Geodata Package (Divisions, Districts, Upazilas/Thanas, and more) for any PHP app (Laravel-ready).",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Your Name",
      "email": "you@example.com"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "Nayemuf\\BdGeo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nayemuf\\BdGeo\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nayemuf\\BdGeo\\Laravel\\BdGeoServiceProvider"
      ],
      "aliases": {
        "BdGeo": "Nayemuf\\BdGeo\\Laravel\\Facades\\BdGeo"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}


